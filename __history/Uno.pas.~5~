unit Uno;

interface
uses system.IOUtils,System.SysUtils,UFRameFracao;
type
  TNo=class
  public
    fracao:TFrame_Fracao;
    dir:TNo;
    esq:TNO;
    constructor Create(Avalor:double); overload;
  end;
implementation

{ TNo }

constructor TNo.Create(Avalor:double);
begin

  valor:=Avalor;
  esq:=nil;
  dir:=nil;
end;

end.
